{
  "expo": {
    "name": "Credify",
    "slug": "ronney",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "credify",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.credifyapp.credify",
      "usesAppleSignIn": true,
      "requireFullScreen": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "credify"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "dunkindonuts",
          "uber",
          "ubereats",
          "doordash",
          "grubhub",
          "resy",
          "otherapp"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIStatusBarHidden": false,
        "UIStatusBarStyle": "UIStatusBarStyleDarkContent",
        "UIViewControllerBasedStatusBarAppearance": false,
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library so you can select a profile picture.",
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {}
        }
      },
      "entitlements": {
        "aps-environment": "production",
        "com.apple.developer.applesignin": [
          "Default"
        ]
      },
      "jsEngine": "hermes",
      "hermesBuildFlags": [
        "-Os",
        "-w"
      ],
      "hermesFlags": {
        "gc": "heuristic",
        "small-global-tables": true,
        "track-io": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.credifyronney.credify",
      "allowBackup": true,
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "credify",
              "host": "*",
              "pathPrefix": "/auth/callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "jsEngine": "hermes",
      "hermesBuildFlags": [
        "-Os",
        "-w"
      ],
      "hermesFlags": {
        "gc": "heuristic",
        "small-global-tables": true,
        "track-io": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-apple-authentication",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.4",
            "useFrameworks": "static"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "extraProguardRules": "-keep class com.credifyapp.credify.BuildConfig { *; }",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResources": true
          }
        }
      ],
      [
        "sentry-expo",
        {
          "enableInExpoDevelopment": true,
          "enableAutoPerformanceTracking": true,
          "uploadSourceMaps": true,
          "hooks": {
            "postPublish": {
              "includePaths": [
                "app/**",
                "components/**",
                "lib/**"
              ]
            }
          }
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "75e33702-056a-43e1-a206-50e6b8d63b05"
      },
      "SUPABASE_URL": "https://ozgnkpadloshnwliaodw.supabase.co",
      "SUPABASE_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im96Z25rcGFkbG9zaG53bGlhb2R3Iiwicm9sZSI6ImFub24iLCJpYXQiOjE3NDgwMjYzMjAsImV4cCI6MjA2MzYwMjMyMH0.PyD1EYJbZcR_SCfTRuye2cOqnLCHl_44vitM9kGWm9w"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 30000
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "credifyapp"
  }
}
